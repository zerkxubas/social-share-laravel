{
    "name": "zerkxubas/social-share-laravel",
    "description": "A simple & effective laravel package for social link sharing",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zerkxubas\\SocialShareLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Subash Sharma",
            "email": "zerkxubas@gmail.com",
            "homepage": "https://zerkbro.com"
        }
    ],
    "keywords": [
        "php",
        "laravel",
        "social",
        "share",
        "links",
        "social link share",
        "link share",
        "nepal"
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zerkxubas\\SocialShareLaravel\\SocialShareServiceProvider"
            ],
            "aliases": {
                "SocialShare": "Zerkxubas\\SocialShareLaravel\\Facades\\SocialShare"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    }
}
